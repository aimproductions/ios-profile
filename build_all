rm -rf build
mkdir build

xcodebuild -configuration Release -sdk iphoneos7.1 -project submodules/soomla-ios-core/SoomlaiOSCore.xcodeproj -target SoomlaiOSCore clean build CREATING_UNIVERSAL_DIR=$PWD/build
xcodebuild -configuration Release -sdk iphoneos7.1 -project SoomlaiOSProfile.xcodeproj -target SoomlaiOSProfile clean build CREATING_UNIVERSAL_DIR=$PWD/build

xcodebuild -configuration Release -sdk iphoneos7.1 -project social-providers/ios-profile-facebook/SoomlaiOSProfileFacebook.xcodeproj -target SoomlaiOSProfileFacebook clean build CREATING_UNIVERSAL_DIR=$PWD/build
